{
  "functions": {
    "api/users/auth.js": {
      "maxDuration": 30
    },
    "api/pots/manage.js": {
      "maxDuration": 30
    }
  },
  "env": {
    "SUPABASE_URL": "@supabase-url",
    "SUPABASE_SERVICE_ROLE_KEY": "@supabase-service-role-key",
    "JWT_SECRET": "@jwt-secret"
  },
  "build": {
    "env": {
      "NEXT_TELEMETRY_DISABLED": "1"
    }
  },
  "regions": ["fra1"],
  "cleanUrls": true,
  "trailingSlash": false
}